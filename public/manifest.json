{
	"$schema": "https://json.schemastore.org/web-manifest-combined",
	"name": "Dankcord",
	"description": "A completely original chat app.",
	"lang": "en-US",
	"start_url": "/channels",
	"display": "standalone",
	"background_color": "#23272a",
	"theme_color": "#2c2f33",
	"categories": ["chat", "memes", "social"],
	"scope": "/",
	"icons": [
		{
			"src": "/icons/manifest-icon-192.png",
			"sizes": "192x192",
			"type": "image/png",
			"purpose": "maskable any"
		},
		{
			"src": "/icons/manifest-icon-512.png",
			"sizes": "512x512",
			"type": "image/png",
			"purpose": "maskable any"
		}
	]
}
