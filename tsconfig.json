{
	// This is just for VSCode stuff I think
	"compilerOptions": {
		"module": "CommonJS",
		"target": "ES5",
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"jsx": "react",
		"strict": true,
		"strictNullChecks": true,
		"noImplicitAny": true,
		"baseUrl": ".",
		"paths": {
			"@assets/*": ["src/app/assets/*"],
			"@components/*": ["src/app/components/*"]
		}
	}
}
